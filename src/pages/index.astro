---
import MainLayout from '../layouts/MainLayout.astro';
import Pokemons from '../components/Pokemons.astro';

const pokemons = await fetch('https://raw.githubusercontent.com/jherr/pokemon/main/index.json');
const pokemons_json = await pokemons.json();
const pokemons_json_10_first = pokemons_json.filter( (p) => p.id < 11 );
---

<MainLayout name="Pokemon List">
    <!---<PD {...data_json} />-->
		<Pokemons pokemons={pokemons_json_10_first} />
</MainLayout>